{
  "name": "sequelize-logbook",
  "version": "0.0.1",
  "description": "A plugin for adding a log book / revision history / audit / paper trail to a sequelize model.",
  "main": "index.js",
  "scripts": {
    "prettier": "find ./ -name '*.js' -not -path './node_modules/*' | xargs ./node_modules/.bin/prettier --write --single-quote --trailing-comma es5",
    "test": "mocha",
    "coverage": "nyc --temp-dir='./coverage/.nyc_output' --reporter=html --reporter=text-summary npm run test"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "prettier": "^1.7.2",
    "proxyquire": "^1.8.0",
    "sqlite3": "^4.1.1"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "sequelize": "^5.21.3"
  },
  "keywords": [
    "sequelize",
    "sequelizejs",
    "sequelize-plugin",
    "revision",
    "revisions",
    "versions",
    "versioning",
    "history",
    "audit",
    "audit trail",
    "trail",
    "paper trail",
    "papertrail",
    "revision"
  ],
  "author": "",
  "license": "MIT"
}
